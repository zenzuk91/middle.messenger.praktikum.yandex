  "compilerOptions": {
    "outDir": "./dist",
    "target": "es2017",
    "declaration": false,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": true,
    "typeRoots": ["node_modules/@types", "typings/app.d.ts"],
    "moduleResolution": "classic",
  }
}